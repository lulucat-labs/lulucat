{
  "name": "@lulucat/service",
  "version": "1.0.0",
  "description": "lulucat service",
  "author": "lulucat labs",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild:prod": "npx rimraf dist",
    "build": "nest build && pnpm run obfuscate",
    "obfuscate": "node scripts/obfuscate.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "pnpm run start:local",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main.js",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/typeorm.config.ts",
    "test:proxy-ips": "ts-node test/test-proxy-ips.ts",
    "import:task-results": "ts-node scripts/import-task-results.ts",
    "update:account-group-resources": "ts-node scripts/update-account-group-resources.ts",
    "decrypt-db": "ts-node src/config/decrypt-db-data.ts",
    "sync-account-group-items": "ts-node scripts/sync-account-group-items.ts",
    "batch-modify-proxy-types": "ts-node scripts/batch-modify-proxy-types.ts",
    "export:user-wallets": "ts-node scripts/export-user-wallets.ts",
    "update-wallet-tasks": "ts-node scripts/check-wallet-balances.ts"
  },
  "dependencies": {
    "@lulucat/exceptions": "workspace:*",
    "@lulucat/tdk": "workspace:*",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/typeorm": "^10.0.1",
    "@types/better-queue": "^3.8.6",
    "@types/cron": "^2.4.3",
    "@types/lodash": "^4.17.16",
    "@types/web3": "^1.2.2",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "better-queue": "^3.8.12",
    "cache-manager": "^6.3.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.5.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "https-proxy-agent": "^5.0.0",
    "imap": "^0.8.19",
    "lodash": "^4.17.21",
    "mailparser": "^3.7.2",
    "mysql2": "^3.12.0",
    "nest-winston": "^1.10.2",
    "node-machine-id": "^1.1.12",
    "patchright": "^1.52.4",
    "playwright": "^1.50.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "terser": "^5.39.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.1.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
