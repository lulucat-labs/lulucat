  "dependencies": {
    "@lulucat/exceptions": "workspace:*",
    "@reservoir0x/relay-sdk": "^1.6.10",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "ethers": "^6.11.5",
    "playwright": "^1.43.0",
    "tslib": "^2.8.1",
    "viem": "^2.23.13",
