  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./wallet": {
      "types": "./dist/wallet/index.d.ts",
      "default": "./dist/wallet/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./defi": {
      "types": "./dist/defi/index.d.ts",
      "default": "./dist/defi/index.js"
    },
    "./network": {
      "types": "./dist/network/index.d.ts",
      "default": "./dist/network/index.js"
    },
    "./social-media": {
      "types": "./dist/social-media/index.d.ts",
      "default": "./dist/social-media/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
